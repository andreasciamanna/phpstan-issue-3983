{
    "name": "wpml/core",
    "description": "WPML Core",
    "license": "GPL-2.0-only",
    "type": "wordpress-plugin",
    "require": {
        "wpml/collect": "dev-hotfix/wpmlga-766",
        "wpml/core-api": "dev-hotfix/wpmlga-766",
        "wpml/fp": "dev-hotfix/wpmlga-766",
        "wpml/wp": "dev-hotfix/wpmlga-766",
        "otgs/ui": "dev-master"
    },
    "autoload": {
        "classmap": [
            "classes/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^0.12.50",
        "phpstan/extension-installer": "^1.0",
        "szepeviktor/phpstan-wordpress": "^0.6.6"
    }
}
